<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@id/exampleListFragment"
    android:label="navigation">

    <fragment
        android:id="@+id/exampleListFragment"
        android:name="com.black.code.ui.example.ExampleListFragment"
        android:label="CodeCollection" >
        <action
            android:id="@+id/action_exampleListFragment_to_retrofitFragment"
            app:destination="@id/retrofitFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_usageTimerFragment"
            app:destination="@id/usageTimerFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_serviceFragment"
            app:destination="@id/serviceFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_launcherFragment"
            app:destination="@id/launcherFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_architectureFragment"
            app:destination="@id/architectureFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_textEditorFragment"
            app:destination="@id/textEditorFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_studyPopupFragment"
            app:destination="@id/studyPopupFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_recyclerViewFragment"
            app:destination="@id/recyclerViewFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_alarmFragment"
            app:destination="@id/alarmFragment" />
        <action
            android:id="@+id/action_exampleListFragment_to_etcFragment"
            app:destination="@id/etcFragment" />
    </fragment>

    <fragment
        android:id="@+id/recyclerViewFragment"
        android:name="com.black.code.ui.example.recyclerview.RecyclerViewFragment"
        android:label="RecyclerView"
        tools:layout="@layout/fragment_recyclerview" />

    <fragment
        android:id="@+id/retrofitFragment"
        android:name="com.black.code.ui.example.retrofit.RetrofitFragment"
        android:label="Retrofit"
        tools:layout="@layout/fragment_retrofit" />

    <fragment
        android:id="@+id/serviceFragment"
        android:name="com.black.code.ui.example.service.ServiceFragment"
        android:label="Service"
        tools:layout="@layout/fragment_service" />


    <fragment
        android:id="@+id/alarmFragment"
        android:name="com.black.code.ui.example.alarm.AlarmFragment"
        android:label="Alarm"
        tools:layout="@layout/fragment_alarm" />

    <fragment
        android:id="@+id/architectureFragment"
        android:name="com.black.code.ui.example.architecture.ArchitectureFragment"
        android:label="Architecture"
        tools:layout="@layout/fragment_architecture" />

    <fragment
        android:id="@+id/launcherFragment"
        android:name="com.black.code.ui.example.launcher.LauncherFragment"
        android:label="Launcher"
        tools:layout="@layout/fragment_launcher" />


    <fragment
        android:id="@+id/studyPopupFragment"
        android:name="com.black.code.ui.example.studypopup.StudyPopupFragment"
        android:label="StudyPopup"
        tools:layout="@layout/fragment_study_popup" />

    <fragment
        android:id="@+id/textEditorFragment"
        android:name="com.black.code.ui.example.texteditor.TextEditorFragment"
        android:label="TextEditor"
        tools:layout="@layout/fragment_text_editor" />

    <fragment
        android:id="@+id/usageTimerFragment"
        android:name="com.black.code.ui.example.usagetimer.UsageTimerFragment"
        android:label="UsageTimer"
        tools:layout="@layout/fragment_usage_timer" />

    <fragment
        android:id="@+id/etcFragment"
        android:name="com.black.code.ui.example.etc.ETCFragment"
        android:label="ETC"
        tools:layout="@layout/fragment_etc" />

    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.black.code.ui.example.notification.NotificationFragment"
        android:label="Notification" />

</navigation>